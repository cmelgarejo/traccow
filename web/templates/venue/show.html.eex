<h2>Show venue</h2>

<ul>

  <li>
    <strong>Code:</strong>
    <%= @venue.code %>
  </li>

  <li>
    <strong>Name:</strong>
    <%= @venue.name %>
  </li>

  <li>
    <strong>Lat:</strong>
    <%= @venue.lat %>
  </li>

  <li>
    <strong>Lon:</strong>
    <%= @venue.lon %>
  </li>

  <li>
    <strong>Detection radius:</strong>
    <%= @venue.detection_radius %>
  </li>

</ul>

<%= link "Edit", to: venue_path(@conn, :edit, @venue) %>
<%= link "Back", to: venue_path(@conn, :index) %>
